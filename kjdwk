<script>
    $(document).ready(function () {
        $(".bars,.nav ul li a").click(function () {
            $('.nav').toggleClass("open")
            $(".bars").toggleClass("active");
        })
        $(".nav ul li a, .go-down").click(function (event) {
            if (this.hash !== "") {

                event.preventDefault();

                var hash = this.hash;

                $('html,body').animate({
                    scrollTop: $(hash).offset().top
                }, 800, function () {
                    window.location.hash = hash;
                });

                // add active class in navigation
                $(".nav ul li a").removeClass("active")
                $(this).addClass("active")
            }
        })
    })
</script>

<a href="javascript:void(0)" class="bars">
    <span></span>
    <span></span>
</a>
<div class="nav">
    <ul>
        <li><a href="#home"><i class="fas fa-home"></i> Home</a></li>
        <li><a href="#about"><i class="fas fa-info-circle"></i> About</a></li>
        <li><a href="#services"><i class="fas fa-clipboard-list"></i> Services</a></li>
        <li><a href="#schedule"><i class="far fa-clock"></i> Schedule</a></li>
        <li><a href="#gallery"><i class="far fa-images"></i> Gallery</a></li>
        <li><a href="#trainer"><i class="fas fa-dumbbell"></i> Trainer</a></li>
        <li><a href="#price"><i class="fas fa-dollar-sign"></i> Price</a></li>
        <li><a href="#contact"><i class="fas fa-phone-alt"></i> Contact</a></li>
    </ul>
</div>



